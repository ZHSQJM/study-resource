<scroll-view scroll-y class="scrollPage">
  <view class='nav-list'>
    <navigator open-type="navigate" hover-class='none' url="/pages/list/list?id={{item.id}}" class="nav-li bg-{{item.color}}" wx:for="{{elements}}" wx:key>
      <view class="nav-title">{{item.name}}</view>
      <view class="nav-name">{{item.description}}</view>
      <text class='cuIcon-{{item.icon}}'></text>
    </navigator>
	
	<navigator open-type="navigate" hover-class='none' url="/pages/list/list?id=1" class="nav-li bg-black">
	  <view class="nav-title">测试</view>
	  <view class="nav-name">测试</view>
	  <text class='cuIcon-{{item.icon}}'></text>
	</navigator>
  </view>
  <view class='cu-tabbar-height'></view>
</scroll-view>

